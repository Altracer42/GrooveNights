<ActorFrame 
OnCommand="zoom,0.3;x,-2;y,6;wag;EffectMagnitude,0,0,2;" 
InitCommand="%function(self) self:hibernate(0.01); self:queuecommand('PlaySound') end"
PlaySoundCommand="%function(self) self:hibernate(gnOnScreenSeconds); end">
